<div>
<%= link_to(medication_group_path(medication_group.id)) do %>
  <div class="mt-2 bg-teal-100 rounded-3xl">
    <div class="mx-6 py-4">
      <h1 class="text-xl font-semibold text-dark-65">
        <%= medication_group.group_name %>
      </h1>

      <div class="mt-2 ml-2">
        <% medication_group.users.each do |user| %>
          <p class=" text-dark-65">
            <%= user.user_name %> 
          </p>
        <% end %>
      </div>
    </div>
  </div>
<% end %>
</div>